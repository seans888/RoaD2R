{
  "manifest_version": 2,

  "name": "Project Wyatt",
  "description": "This extensions identifies fake news on your facebook news feed",
  "version": "0.1",

  "browser_action": {
    "default_icon": "proof.png",
    "default_popup": "popup.html"
  },
    "icons":{
        "64" : "proof.png"
    },
    "content_scripts": [
      {
         "matches": ["https://www.facebook.com/*"],
         "css": ["/ladda/ladda.css"],
         "js": [ "myScript.js", "/ladda/ladda.js", "/ladda/spin.js"]
      }
    ]
}
